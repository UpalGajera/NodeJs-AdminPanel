<%- include('header') %>
    <div class="container-scroller">
        <div class="content-wrapper d-flex  auth px-0">
            <div class="auth-form-light text-left py-5 px-4 px-sm-5">
                <div class="brand-logo">
                    <h1>SubCategory</h1>
                </div>
                <form class="pt-3" method="post" action="/subcategoryData">
                    <div class="form-group">
                        <div class="d-flex">
                        <label class="col-sm-3 text-right control-label col-form-label">Category</label>
                            <select name="categoryId" class="form-control">
                                <option>---select category---</option>
                                <% categorydata.map((val)=> { %>

                                    <option value="<%= val.id %>">
                                        <%= val.category %>
                                    </option>
                                    <% }) %>
                            </select>
                        </div>
                        <div class="form-group">
                            <input type="text" name="subcategory" class="form-control form-control-lg"
                                placeholder="Enter Your SubCategory">
                        </div>
                        <div class="mt-3">
                            <input value="ADD" type="submit"
                                class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn">
                        </div>

                </form>
            </div>
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <%- include('footer') %>