<%- include('header') %>
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Category</th>
                            <th>SubCategory</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% 
                                let subcat = 0;
                                allrecord.map((val)=>{ 
                                subcat++;
                            %>
                                <tr>
                                    <td><%= subcat %></td>
                                    <td><%= val.categoryId.category %></td>
                                    <td><%= val.subcategory %></td>
                                </tr>
                            <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<%- include('footer') %>